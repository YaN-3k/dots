def_key "j"
    scroll_down

def_key "k"
    scroll_up

def_key "h"
    previous_column

def_key "l"
    next_column

def_key "l"
  enter_directory

def_key "l"
  run_action

def_key "l"
  play_item

def_key "h"
  jump_to_parent_directory

def_key "ctrl-u"
  page_up

def_key "ctrl-d"
  page_down

def_key "G"
  move_end

def_key "g"
  move_home

def_key "K"
  select_item
  scroll_up

def_key "J"
  select_item
  scroll_down

def_key "d"
  delete_playlist_items

def_key "d"
  delete_stored_playlist

def_key "d"
  delete_browser_items
